<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite>
    <rule match-type="regex">
        <condition type="request-uri" operator="notequal">^/\w*/rest/.*$</condition>
        <condition type="request-uri" operator="notequal">^/\w*/ace/.*$</condition>
        <condition type="request-uri" operator="notequal">^/\w*/index.html$</condition>
        <condition type="request-uri" operator="notequal">^/.*\.js$</condition>
        <from>^.*$</from>
        <to>index.html</to>
    </rule>
</urlrewrite>